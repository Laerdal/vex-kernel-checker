# Test kernel config WITH mixed filesystem support
CONFIG_NET=y
CONFIG_INET=y
CONFIG_BLOCK=y
CONFIG_PROC_FS=y
CONFIG_SYSFS=y

# Enable EXT4 and BTRFS, but not SMB/CIFS or NFS
CONFIG_EXT4_FS=y
CONFIG_EXT4_USE_FOR_EXT2=y
CONFIG_BTRFS_FS=y

# Disable SMB/CIFS
# CONFIG_CIFS is not set

# Disable NFS
# CONFIG_NFS_FS is not set
