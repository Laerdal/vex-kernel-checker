# Test kernel configuration for VEX Kernel Checker
CONFIG_NET=y
CONFIG_USB=y
CONFIG_PCI=y
CONFIG_ARM64=y
CONFIG_ARM64_IMX8MP=y
CONFIG_SCSI=y
CONFIG_BLOCK=y
CONFIG_FILESYSTEMS=y
CONFIG_SOUND=y
# DEBUG options (should be filtered out)
CONFIG_DEBUG_KERNEL=y
CONFIG_DEBUG_INFO=y
# XEN options (should be filtered out for non-XEN)
CONFIG_XEN=n
CONFIG_XEN_BACKEND=n
